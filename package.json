{
  "name": "homedical",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc:w": "tsc --watch",
    "clean": "rm -rf ./build && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rimraf $TMPDIR/react-* && watchman watch-del-all && rimraf ios/build && rimraf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "build-android-development": "export ENVFILE=.env.development && cd android && ./gradlew assembleDev",
    "fastlane:android:internal": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && fastlane internal",
    "fastlane:android:staging": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && fastlane staging",
    "fastlane:android:production": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && fastlane production",
    "fastlane:ios:production": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios && cd ios && pod update && pod install && fastlane productionHomedical",
    "fastlane:ios:staging": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios && cd ios && pod update && pod install && fastlane stagingHomedical",
    "fastlane:ios:internal": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios && cd ios && pod update && pod install && fastlane internalHomedical",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s && npm run test -s",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "environment": "chmod +x env.sh && ./env.sh",
    "postinstall": "node ./android-release-gradle-fix.js;"
  },
  "dependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.41",
    "@types/react-native": "^0.52.19",
    "apisauce": "0.14.3",
    "format-json": "1.0.3",
    "geolib": "^3.0.4",
    "identity-obj-proxy": "3.0.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "prop-types": "15.6.1",
    "querystringify": "1.0.0",
    "ramda": "0.25.0",
    "react": "16.8.3",
    "react-moment": "^0.7.0",
    "react-native": "0.62.3",
    "react-native-animatable": "^1.2.4",
    "react-native-config": "^0.11.7",
    "react-native-fabric": "^0.5.1",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fused-location": "^0.5.0",
    "react-native-i18n": "^2.0.12",
    "react-native-image-picker": "^0.28.1",
    "react-native-keyboard-aware-scroll-view": "0.8.0",
    "react-native-masked-text": "^1.6.5",
    "react-native-orientation-locker": "^1.1.6",
    "react-native-responsive-dimensions": "^2.0.1",
    "react-native-root-toast": "^3.0.1",
    "react-native-router-flux": "^3.38.0",
    "react-native-signature-pad": "^0.1.0",
    "react-native-simple-dialogs": "^1.2.0",
    "react-native-uuid": "^1.4.9",
    "react-native-vector-icons": "4.3.0",
    "react-native-webview": "^5.12.1",
    "react-redux": "5.0.7",
    "react-redux-typescript": "^2.3.0",
    "realm": "^2.29.1",
    "redux": "3.7.2",
    "redux-persist": "5.9.1",
    "redux-saga": "0.16.0",
    "reduxsauce": "0.7.0",
    "rxjs": "^5.5.8",
    "seamless-immutable": "7.1.3",
    "tcomb-form-native": "0.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@storybook/addon-storyshots": "3.3.15",
    "@storybook/addons": "3.3.15",
    "@storybook/channels": "3.3.15",
    "@storybook/react-native": "3.3.15",
    "@types/lodash": "^4.14.108",
    "@types/react-native-fetch-blob": "^0.10.5",
    "@types/react-native-i18n": "^2.0.0",
    "@types/react-redux": "^5.0.16",
    "awesome-typescript-loader": "^3.1.3",
    "babel-jest": "^24.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "3.0.2",
    "buffer": "5.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "ignite-animatable": "^1.0.0",
    "ignite-ir-boilerplate": "^2.3.1",
    "ignite-jhipster": "^1.10.0",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.1.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "mockery": "2.1.0",
    "react-dom": "16.3.0-alpha.1",
    "react-native-typescript-transformer": "1.2.12",
    "react-test-renderer": "16.8.3",
    "reactotron-react-native": "^3.4.0",
    "reactotron-redux": "1.13.0",
    "reactotron-redux-saga": "1.13.0",
    "rimraf": "2.6.2",
    "snazzy": "^7.1.1",
    "standard": "10.0.2",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.8.3"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./Tests/Setup"
    ],
    "coveragePathIgnorePatterns": [
      "App/Services/Api.js",
      "App/Themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "preset": "react-native"
  },
  "config": {}
}
